{
  "name": "@zerowastemap/carto",
  "version": "1.0.0",
  "description": "Zero Waste Map",
  "main": "index.js",
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "example": "bankai start ./example/index.js",
    "test": "standard && npm run test-deps",
    "test-browser": "browserify -t [babelify] test/browser/index.js | tape-run | tap-format-spec",
    "test-chrome": "browserify -t [babelify] test/browser/index.js | tape-run --browser chrome --port 8084 --node | tap-format-spec",
    "test-pupeeteer": "NODE_ENV=test node test/browser/puppeteer.js | tap-format-spec",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/depackt/depackt-leaflet.git"
  },
  "keywords": [
    "nanocomponent",
    "leaflet",
    "zero waste"
  ],
  "author": "Augustin Godiscal <hello@auggod.tech>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/depackt/depackt-leaflet/issues"
  },
  "homepage": "https://github.com/depackt/depackt-leaflet#readme",
  "dependencies": {
    "leaflet": "^1.3.4",
    "leaflet.locatecontrol": "^0.63.0",
    "leaflet.markercluster": "^1.4.1",
    "nanocomponent": "^6.5.2",
    "nanohtml": "^1.2.6",
    "on-idle": "^3.1.4"
  },
  "devDependencies": {
    "@tap-format/spec": "^0.2.0",
    "bankai": "^9.15.0",
    "bufferutil": "^4.0.0",
    "choo": "^6.13.0",
    "dependency-check": "^3.2.1",
    "puppeteer": "^1.9.0",
    "sheetify": "^7.3.3",
    "standard": "^12.0.1",
    "tape": "^4.9.1",
    "tape-run": "^4.0.0",
    "utf-8-validate": "^5.0.1"
  }
}
